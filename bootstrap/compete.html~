<script>
$(document).ready(function(){
//updates
$(".updates ul").css({'list-style':"none"});
$(".updates ul li a").css({'color':"gold", 'font-family':"GoodDog",'text-decoration':"none"});
$(".updates ul li a").hover(function(){
$(this).css({'color':"white"}).addClass("shake-class");
},function(){
$(".updates ul li a").css({'color':"gold"}).removeClass("shake-class");
});

//login-form
$("#loginNow").click(function(){
$(".login-form").css({'opacity':"1",'webkit-transform':"scale(1)",'-moz-transform':"scale(1)"},"slow");
$(".container").css({'-webkit-filter':"grayscale(50%) blur(5px)",'-moz-filter':"grayscale(50%) blur(5px)"});
});
$(".login-form .btn-login").click(function(){
$(".login-form").css({'opacity':"0",'-webkit-transform':"scale(0)"},"slow");
$(".container").css({'-webkit-filter':"grayscale(0%)",'-moz-filter':"grayscale(0%)"});
});



//info-box

$("#info").click(function(){
$(".info-box").css({'opacity':"1",'webkit-transform':"scale(1)",'-moz-transform':"scale(1)"},"slow");
$(".container").css({'-webkit-filter':"grayscale(50%) blur(5px)",'-moz-filter':"grayscale(50%) blur(5px)"});
});
$(".info-box .btn-x").click(function(){
$(".info-box").css({'opacity':"0",'-webkit-transform':"scale(0)",'-moz-transform':"scale(0)"},"slow");
$(".container").css({'-webkit-filter':"grayscale(0%)"});
});

//contest-box

$("#contest").click(function(){
$(".contest-box").css({'opacity':"1",'webkit-transform':"scale(1)"},"slow");
$(".container").css({'-webkit-filter':"grayscale(50%) blur(5px)"});
});
$(".contest-box .btn-c-x").click(function(){
$(".contest-box").css({'opacity':"0",'-webkit-transform':"scale(0)",'-moz-transform':"scale(0)",'transform':"scale(0)"},"slow");
$(".container").css({'-webkit-filter':"grayscale(0%)"});
});


});
</script>
